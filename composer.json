{
    "name": "state/og-generator",
    "autoload": {
        "psr-4": {
            "State\\OgGenerator\\": "src"
        }
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Christian Pav",
            "email": "christian@statebuilt.com",
            "homepage": "https://statebuilt.com",
            "role": "Developer"
        }
    ],
    "autoload-dev": {
        "psr-4": {
            "OgGeneratorTests\\": "tests/"
        }
    },
    "extra": {
        "statamic": {
            "name": "OG Generator",
            "description": "Generate OG images from your page's title."
        },
        "laravel": {
            "providers": [
                "State\\OgGenerator\\ServiceProvider"
            ]
        }
    },
    "require-dev": {
        "pestphp/pest": "^2.34",
        "orchestra/testbench": "^9.1"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "pixelfear/composer-dist-plugin": true
        }
    },
    "require": {
        "intervention/image": "^2.7",
        "illuminate/support": ">=10",
        "statamic/cms": "^5.7"
    }
}
